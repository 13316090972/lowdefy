  "repository": {
    "type": "git",
    "url": "https://github.com/lowdefy/lowdefy.git"
  },
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./blocks/*": "./dist/blocks/*",
    "./style.less": "./dist/style.less"
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "yarn swc",
    "clean": "rm -rf dist",
