    "antd": "4.17.0-alpha.7",
    "classnames": "2.3.1",
    "moment": "2.29.1",
    "rc-animate": "3.1.1",
    "react": "18.0.0-alpha-327d5c484-20211106",
    "react-dom": "18.0.0-alpha-327d5c484-20211106",
    "react-router-dom": "5.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/preset-react": "7.16.0",
    "@emotion/jest": "11.5.0",
    "@lowdefy/block-dev": "3.22.0",
    "@swc/cli": "0.1.51",
    "@swc/core": "1.2.107",
