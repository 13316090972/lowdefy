    "swc": "swc src --out-dir dist --config-file ../../../../.swcrc --delete-dir-on-start --copy-files",
    "test:watch": "jest --coverage --watch",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@ant-design/icons": "4.7.0",
    "@lowdefy/block-utils": "3.22.0",
