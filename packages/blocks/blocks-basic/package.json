  "repository": {
    "type": "git",
    "url": "https://github.com/lowdefy/lowdefy.git"
  },
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./blocks/*": "./dist/blocks/*"
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "yarn swc",
    "clean": "rm -rf dist",
    "prepare": "yarn build",
    "swc": "swc src --out-dir dist --config-file ../../../.swcrc --delete-dir-on-start --copy-files",
    "test:watch": "jest --coverage --watch",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@lowdefy/block-utils": "3.22.0",
    "@lowdefy/helpers": "3.22.0",
    "dompurify": "2.3.3",
