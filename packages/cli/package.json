{
  "name": "@lowdefy/cli",
  "version": "3.0.0-experimental.1",
  "license": "Apache-2.0",
  "description": "Lowdefy CLI",
  "homepage": "https://lowdefy.com",
  "bugs": {
    "url": "https://github.com/lowdefy/lowdefy/issues"
  },
  "contributors": [
    {
      "name": "Sam Tolmay",
