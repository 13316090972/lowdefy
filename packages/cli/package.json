    },
    {
      "name": "Gerrie van Wyk",
      "url": "https://github.com/Gervwyk"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lowdefy/lowdefy.git"
  },
  "bin": {
    "lowdefy": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn webpack",
    "clean": "rm -rf dist",
    "cli": "yarn node ./dist/index.js",
    "cli:build": "yarn build && yarn node ./dist/index.js",
    "npm-publish": "npm publish --access public",
    "prepare": "yarn build",
    "prepublishOnly": "yarn build",
    "test": "jest --coverage --passWithNoTests",
    "webpack": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "@lowdefy/build": "0.0.0-experimental.0",
    "@lowdefy/helpers": "1.1.0",
    "@lowdefy/node-utils": "1.0.0",
    "chalk": "4.1.0",
    "chokidar": "3.4.2",
    "commander": "6.1.0",
    "inquirer": "7.3.3"
  },
  "devDependencies": {
    "@babel/cli": "7.12.1",
    "@babel/core": "7.12.3",
    "@babel/preset-env": "7.12.1",
    "babel-jest": "26.5.2",
    "babel-loader": "8.1.0",
    "jest": "26.5.3",
    "webpack": "5.1.3",
    "webpack-cli": "4.0.0"
  }
