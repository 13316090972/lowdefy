{
  "category": "display",
  "loading": false,
  "schema": {
    "properties": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "code": {
          "type": "boolean",
          "default": false,
          "description": "Apply code style."
        },
        "color": {
          "type": "string",
          "description": "Title color."
        },
        "content": {
          "type": "string",
          "description": "Title text content."
        },
        "copyable": {
          "type": "boolean",
          "default": false,
          "description": "Provide copy text button."
        },
        "delete": {
          "type": "boolean",
          "default": false,
          "description": "Apply deleted (strikethrough) style."
        },
        "disabled": {
          "type": "boolean",
          "default": false,
          "description": "Apply disabled style."
        },
        "ellipsis": {
          "type": "boolean",
          "default": false,
          "description": "Display ellipsis when text overflows."
        },
        "level": {
          "type": "number",
          "default": 1,
          "enum": [1, 2, 3, 4],
          "description": "Set title type. Matches with h1, h2, h3 and h4."
        },
        "mark": {
          "type": "boolean",
          "default": false,
          "description": "Apply marked (highlighted) style."
        },
        "type": {
          "type": "string",
          "enum": ["secondary", "warning", "danger"],
          "description": "Additional types. Don't specify for default."
        },
        "underline": {
          "type": "boolean",
          "default": false,
          "description": "Apply underline style."
        }
      }
    }
  }
}
