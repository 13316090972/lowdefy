{
  "category": "container",
  "loading": false,
  "schema": {
    "properties": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "accordion": {
          "type": "boolean",
          "description": "If true, only one panel is open at a time."
        },
        "activeKey": {
          "type": "string",
          "description": "Current panel's key."
        },
        "bordered": {
          "type": "boolean",
          "description": "Toggles rendering of the border around the collapse block."
        },
        "defaultActiveKey": {
          "type": "string",
          "description": "Initial active panel's key, if activeKey is not set."
        },
        "destroyInactivePanel": {
          "type": "boolean",
          "description": "Destroy inactive panel."
        },
        "expandIcon": {
          "type": ["string", "object"],
          "description": "Icon to show as expand option."
        },
        "expandIconPosition": {
          "type": "string",
          "enum": ["left", "right"],
          "description": "Set position of expand icon."
        },
        "forceRender": {
          "type": "boolean",
          "description": "Force render for all panels."
        },
        "panels": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "title": {
                "type": "string",
                "description": "Title of the panel."
              },
              "key": {
                "type": "string",
                "description": "Key of the panel."
              },
              "extraKey": {
                "type": "string",
                "description": "Key for the extra area of the panel."
              },
              "disabled": {
                "type": "boolean",
                "description": "Disable the panel if true."
              }
            }
          }
        },
        "showArrow": {
          "type": "boolean",
          "description": "Show expand icon."
        }
      }
    },
    "actions": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "onChange": {
          "type": "array"
        }
      }
    }
  }
}
